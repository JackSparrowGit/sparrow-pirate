<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.corsair.sparrow.pirate.oauth.mapper.SysUserMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.corsair.sparrow.pirate.oauth.domain.bean.SysUser">
    <result column="id" property="id" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="salt" property="salt" />
        <result column="first_name" property="firstName" />
        <result column="last_name" property="lastName" />
        <result column="nick_name" property="nickName" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="gender" property="gender" />
        <result column="image" property="image" />
        <result column="is_enabled" property="isEnabled" />
        <result column="is_account_non_expired" property="isAccountNonExpired" />
        <result column="is_account_non_locked" property="isAccountNonLocked" />
        <result column="is_credentials_non_expired" property="isCredentialsNonExpired" />
        <result column="language" property="language" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="region" property="region" />
        <result column="last_login_time" property="lastLoginTime" />
        <result column="primary_key" property="primaryKey" />
        <result column="birthday" property="birthday" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        username, password, salt, first_name, last_name, nick_name, phone, email, gender, image, is_enabled, is_account_non_expired, is_account_non_locked, is_credentials_non_expired, language, create_time, update_time, region, last_login_time, primary_key, birthday
    </sql>

</mapper>
