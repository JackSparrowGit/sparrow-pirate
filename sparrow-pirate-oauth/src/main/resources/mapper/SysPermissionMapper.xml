<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.corsair.sparrow.pirate.oauth.mapper.SysPermissionMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.corsair.sparrow.pirate.oauth.domain.bean.SysPermission">
    <result column="id" property="id" />
        <result column="parent_id" property="parentId" />
        <result column="parent_ids" property="parentIds" />
        <result column="name" property="name" />
        <result column="depth" property="depth" />
        <result column="sort" property="sort" />
        <result column="icon" property="icon" />
        <result column="clazz" property="clazz" />
        <result column="style" property="style" />
        <result column="url" property="url" />
        <result column="type" property="type" />
        <result column="methods" property="methods" />
        <result column="options_fields" property="optionsFields" />
        <result column="protect_type" property="protectType" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
        <result column="is_enable" property="isEnable" />
        <result column="description" property="description" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        parent_id, parent_ids, name, depth, sort, icon, clazz, style, url, type, methods, options_fields, protect_type, create_by, update_by, is_enable, description, create_time, update_time
    </sql>

</mapper>
